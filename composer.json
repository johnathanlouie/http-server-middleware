{
    "name": "lwd/http-server-middleware",
    "description": "Common interface for HTTP server-side middleware",
    "keywords": [
        "psr",
        "psr-7",
        "psr-15",
        "http-interop",
        "http",
        "middleware",
        "request",
        "response"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "PHP-FIG",
            "homepage": "https://www.php-fig.org/"
        }
    ],
    "support": {
        "source": "https://github.com/php-fig/http-server-middleware"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/johnathanlouie/http-message"
        },
        {
            "type": "vcs",
            "url": "https://github.com/johnathanlouie/http-server-handler"
        }
    ],
    "require": {
        "php": "^5.5",
        "lwd/http-message": "^1.0 || ^2.0",
        "lwd/http-server-handler": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Lwd\\Http\\Server\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
